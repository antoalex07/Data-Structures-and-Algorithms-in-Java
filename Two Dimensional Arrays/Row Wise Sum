
public class Solution {

	public static void rowWiseSum(int[][] mat) {
		
        int row = mat.length;
        if(row > 0){
        int col = mat[0].length;
        for(int i = 0; i < row; i++){
            
            int rowsum = 0;
            for(int j = 0; j < col; j++){
                
                rowsum = rowsum + mat[i][j];
            }
            System.out.print(rowsum + " ");
        }
        System.out.println();
        }      
	}
}
