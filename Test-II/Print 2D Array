public class solution {
	public static void print2DArray(int input[][]) {
		
		int row = input.length;
        if(row > 0){
            
            int col = input[0].length;
            for(int i = 0; i < row; i++){
                
                int num = 0;
                while(num < row - i){
                    for(int j = 0; j < col; j++)
                        System.out.print(input[i][j] + " ");
                	System.out.println();
                    num++;
                }
            }
        }
	}
}
