import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
        Scanner s = new Scanner(System.in);
        int N = s.nextInt();
        int rev = 0;
        while(N > 0){
            
            int rem = N % 10;
            rev = rev * 10 + rem;
            N = N / 10;
        }
		System.out.println(rev);
	}
}
