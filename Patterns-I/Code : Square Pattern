import java.util.Scanner;

public class Solution {


	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);
        int N = s.nextInt();
        int i = 0;
        while(i < N){
            
            int j = 0;
            while(j < N){
                
                System.out.print(N);
                j++;
            }
            System.out.println();
            i++;
        }
 		
	}

}
